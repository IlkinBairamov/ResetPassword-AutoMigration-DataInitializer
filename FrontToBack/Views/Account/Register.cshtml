@model RegisterViewModel
@{
    Layout = null;
}

<!doctype html>
<html lang="en">

<!-- Mirrored from bhulua.thememinister.com/register.html by HTTrack Website Copier/3.x [XR&CO'2014], Sat, 12 Oct 2019 13:14:22 GMT -->
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Responsive Bootstrap 4 Admin &amp; Dashboard Template">
    <meta name="author" content="Bdtask">
    <title>Bhulua - Bootstrap 4 Admin Template Deshboard</title>
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Admin/assets/dist/img/favicon.png">
    <!--Global Styles(used by all pages)-->
    <link href="~/Admin/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Admin/assets/plugins/metisMenu/metisMenu.min.css" rel="stylesheet">
    <link href="~/Admin/assets/plugins/fontawesome/css/all.min.css" rel="stylesheet">
    <link href="~/Admin/assets/plugins/typicons/src/typicons.min.css" rel="stylesheet">
    <link href="~/Admin/assets/plugins/themify-icons/themify-icons.min.css" rel="stylesheet">
    <!--Third party Styles(used by this page)-->
    <!--Start Your Custom Style Now-->
    <link href="~/Admin/assets/dist/css/style.css" rel="stylesheet">
</head>
<body class="bg-white">
    <div class="d-flex align-items-center justify-content-center text-center h-100vh">
        <div class="form-wrapper m-auto">
            <div class="form-container my-4">
                <div class="register-logo text-center mb-4">
                    <img src="~/Admin/assets/dist/img/logo2.html" alt="">
                </div>
                <div class="panel">
                    <div class="panel-header text-center mb-3">
                        <h3 class="fs-24">Sign up for your account!</h3>
                        <p class="text-muted text-center mb-0">Start your 15-day trial, no credit card required</p>
                    </div>
                    <a class="btn btn-linkedin btn-block bg-white text-left font-weight-normal position-relative mb-3" href="~/Admin/#">
                        <span>Sign up with LinkedIn</span>
                    </a>
                    <a class="btn  btn-google btn-block bg-white text-left font-weight-normal position-relative mb-3" href="~/Admin/#">
                        <span>Sign up with Google</span>
                    </a>
                    <p class="text-muted text-center">We won't post anything without your permission and your personal details are kept private</p>
                    <div class="divider font-weight-bold text-uppercase text-dark d-table text-center my-3">Or</div>
                    <div class="text-danger" asp-validation-summary=ModelOnly> </div>
                    <form class="register-form" method="post">

                        <div class="form-group" style="text-align:start">
                            <label asp-for="FullName" class="font-weight-600">FullName</label>
                            <input class="form-control" placeholder="FullName" asp-for="FullName">
                            <span asp-validation-for="FullName" class="text-danger">  </span>
                        </div>
                        <div class="form-group" style="text-align:start">
                            <label asp-for="UserName" class="font-weight-600">UserName</label>
                            <input class="form-control" placeholder="UserName" asp-for="UserName">
                            <span asp-validation-for="UserName" class="text-danger">  </span>
                        </div>
                        <div class="form-group" style="text-align:start">
                            <label asp-for="Email" class="font-weight-600">Email</label>
                            <input class="form-control" placeholder="Name" asp-for="Email">
                            <span asp-validation-for="Email" class="text-danger">  </span>
                        </div>
                        <div class="form-group" style="text-align:start">
                            <label asp-for="Password" class="font-weight-600">Password</label>
                            <input class="form-control" placeholder="Password" asp-for="Password">
                            <span asp-validation-for="Password" class="text-danger">  </span>
                        </div>
                        <div class="form-group" style="text-align:start">
                            <label asp-for="ConfirmPassword" class="font-weight-600">ConfirmPassword</label>
                            <input class="form-control" placeholder="ConfirmPassword" asp-for="ConfirmPassword">
                            <span asp-validation-for="ConfirmPassword" class="text-danger">  </span>
                        </div>
                        <div class="custom-control custom-checkbox mb-3">
                            <input type="checkbox" class="custom-control-input" id="customCheck1">
                            <label class="custom-control-label" for="customCheck1">By signing up, you agree to the <a href="~/Admin/#">terms of service</a></label>
                        </div>
                        <button type="submit" class="btn btn-success btn-block">Sign up</button>
                        <p class="text-muted text-center mt-3 mb-0">
                            By signing up, you agree to our <a class="external" href="~/Admin/#">terms of use</a>.
                        </p>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- /.End of form wrapper -->
    <!--Global script(used by all pages)-->
    <script src="~/Admin/assets/plugins/jQuery/jquery-3.4.1.min.js"></script>
    <script src="~/Admin/assets/dist/js/popper.min.js"></script>
    <script src="~/Admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Admin/assets/plugins/metisMenu/metisMenu.min.js"></script>
    <script src="~/Admin/assets/plugins/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <!-- Third Party Scripts(used by this page)-->
    <!--Page Active Scripts(used by this page)-->
    <!--Page Scripts(used by all page)-->
    <script src="~/Admin/assets/dist/js/sidebar.js"></script>
</body>

